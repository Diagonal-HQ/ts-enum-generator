{
    "name": "diagonal/ts-enum-generator",
    "description": "Generate runtime-usable TypeScript objects and enums from PHP enums with utils ",
    "keywords": [
        "diagonal",
        "ts-enum-generator"
    ],
    "homepage": "https://github.com/diagonal/ts-enum-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Branick Weix",
            "email": "bdweix@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Diagonal\\TsEnumGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diagonal\\TsEnumGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
