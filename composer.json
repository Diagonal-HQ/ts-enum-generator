{
  "name": "diagonal/laravel-ts-enum-generator",
  "description": "Generate runtime-usable TypeScript enums from PHP Laravel enums.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "typescript",
    "enum",
    "codegen"
  ],
  "autoload": {
    "psr-4": {
      "Diagonal\\TsEnumGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Diagonal\\TsEnumGenerator\\Tests\\": "tests/",
      "Diagonal\\TsEnumGenerator\\Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": ">=10.0",
    "symfony/finder": "^5.0 || ^6.0",
    "nette/php-generator": "^3.6 || ^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "orchestra/testbench": "^7.0 || ^8.0"
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Diagonal\\TsEnumGenerator\\TsEnumGeneratorServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Branick Weix",
      "email": "branickw@godiagonal.com"
    }
  ]
}